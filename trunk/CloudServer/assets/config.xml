<?xml version="1.0" encoding="utf-8"?>
<config>
	<port>4773</port>
	<services>
		<service>
			<name>File Service</name>
			<class_name>file_service</class_name>
			<library_name>FileService</library_name>
			<settings>
				<root_file_system_directory>htdocs</root_file_system_directory>
				<show_directory_contents>false</show_directory_contents>
			</settings>
			<properties>
				<headers>
					<Accept-Encoding></Accept-Encoding>
				</headers>
			</properties>
		</service>
		<service>
			<name>Cloud Service</name>
			<class_name>cloud_service</class_name>
			<library_name>CloudService</library_name>
			<settings>
				<max_streams>10</max_streams>
				<dumps_location>dumps</dumps_location>
				<dump_writers>false</dump_writers>
				<dump_readers>false</dump_readers>
			</settings>
			<properties>
				<url>
					<equals>/users.json</equals>
				</url>
				<arguments>
					<action>write</action>
					<action>read</action>
				</arguments>
			</properties>
		</service>
	</services>
</config>